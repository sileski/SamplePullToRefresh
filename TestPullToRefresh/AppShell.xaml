<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TestPullToRefresh.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TestPullToRefresh"
    Shell.FlyoutBehavior="Disabled"
    Title="TestPullToRefresh">
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LockPage}"
        Route="LockPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
